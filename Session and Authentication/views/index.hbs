<main>
  <h1>Browser</h1>
  <form action="/search" method="POST">
    <input type="text" class="search" name="search" placeholder="Search...">
    <input type="number" name="from" class="difficulty" placeholder="Difficulty from...">
    <span>-</span>
    <input type="number" name="to" class="difficulty" placeholder="Difficulty to...">
    <input type="submit" value="search">
  </form>
  {{#each cube}}
  <div class="cube">
    <img class="cube" src={{this.imageUrl}}>
    <p class="name">{{this.name}}</p>
    <p><span>Difficulty level:</span> {{this.difficulty}}</p>
    <a class="btn" href="/details/{{this.id}}">Details</a>
  </div>
  {{/each}}

</main>